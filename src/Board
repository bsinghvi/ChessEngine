import java.util.Arrays;

public class Board {
	String[][] b;
	
	public Board() {
		 b = new String[][]{ {"A ROOK", "KNIGHT", "BISHOP", "QUEEN", "KING", "BISHOP", "KNIGHT", "ROOK"},
							 {"PAWN", "PAWN", "PAWN", "PAWN", "PAWN", "PAWN","PAWN", "PAWN"},
							 {"EMPTY", "EMPTY", "EMPTY", "EMPTY", "EMPTY", "EMPTY", "EMPTY", "EMPTY"},
